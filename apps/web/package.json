{
  "name": "@coaching-transcript-tool/frontend",
  "version": "2.25.0",
  "private": true,
  "description": "Next.js frontend for Coaching Transcript Tool",
  "scripts": {
    "dev": "next dev",
    "dev:quiet": "NODE_NO_WARNINGS=1 next dev",
    "build": "next build",
    "build:cf": "npx @opennextjs/cloudflare build",
    "verify:chunks": "node scripts/verify-chunks.js",
    "clean": "rm -rf .next .vercel .turbo",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "deploy": "npm run build && npm run build:cf && npm run verify:chunks && npx wrangler deploy",
    "deploy:only": "npx wrangler deploy",
    "preview": "npx wrangler dev",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@hcaptcha/react-hcaptcha": "^1.12.1",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.3.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "next": "^14.2.3",
    "next-auth": "^4.24.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-recaptcha-v3": "^1.11.0",
    "react-hook-form": "^7.48.0",
    "react-hot-toast": "^2.6.0",
    "tailwind-merge": "^2.0.0",
    "zod": "^3.22.0",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@opennextjs/cloudflare": "^1.6.3",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.2.0"
  },
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "coaching",
    "transcript"
  ],
  "author": "Coaching Transcript Tool Team",
  "license": "ISC"
}
